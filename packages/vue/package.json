{
  "name": "@ultra/icon",
  "version": "1.0.4",
  "description": "Vue components of Icons collection.",
  "type": "module",
  "keywords": [
    "icon",
    "svg",
    "vue"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cabinet-fe/icon/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabinet-fe/icon.git",
    "directory": "packages/vue"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./colorful": {
      "import": "./dist/colorful.js",
      "require": "./dist/colorful.js",
      "types": "./dist/colorful.d.ts"
    },
    "./normal": {
      "import": "./dist/normal.js",
      "require": "./dist/normal.js",
      "types": "./dist/normal.d.ts"
    },
    "./names": {
      "import": "./dist/names.js",
      "require": "./dist/names.js",
      "types": "./dist/names.d.ts"
    },
    "./source": "./src/index.ts",
    "./*": "./*"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bun run build/index.ts"
  },
  "peerDependencies": {
    "vue": ">=^3.4.21"
  },
  "devDependencies": {
    "tsdown": "^0.15.8",
    "icon-svg": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.8.1",
    "camelcase": "^8.0.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.2",
    "prettier": "^3.6.2",
    "unplugin-vue": "^7.0.2",
    "vue": "^3.5.22",
    "vue-tsc": "^3.1.1"
  }
}
