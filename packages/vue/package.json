{
  "name": "icon-ultra",
  "version": "3.0.7",
  "description": "Vue components of Icons collection.",
  "type": "module",
  "keywords": [
    "icon",
    "svg",
    "vue"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cabinet-fe/icon-ultra/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabinet-fe/icon-ultra.git",
    "directory": "packages/vue"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./global": {
      "require": "./dist/global.cjs",
      "import": "./dist/global.js",
      "types": "./dist/global.d.ts"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run build:generate && run-p build:build build:types",
    "build:generate": "tsx build/generate.ts",
    "build:build": "tsx build/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "icon-svg": "workspace:*",
    "@pnpm/find-workspace-dir": "^4.0.3",
    "@pnpm/find-workspace-packages": "^4.0.41",
    "@pnpm/logger": "^4.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.1.2",
    "@types/prettier": "^3.0.0",
    "camelcase": "^7.0.0",
    "chalk": "^5.2.0",
    "consola": "^2.15.3",
    "esbuild": "^0.17.18",
    "esbuild-plugin-globals": "^0.1.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^11.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.3",
    "tsx": "^4.7.2",
    "unplugin-vue": "^4.3.5",
    "vue": "^3.4.21",
    "vue-tsc": "^2.0.6"
  }
}
