{
  "name": "icon-ultra",
  "version": "2.0.15",
  "description": "Vue components of Icons collection.",
  "type": "module",
  "keywords": [
    "icon",
    "svg",
    "vue"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cabinet-fe/icon-ultra/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabinet-fe/icon-ultra.git",
    "directory": "packages/vue"
  },
  "files": [
    "dist"
  ],

  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./global": {
      "require": "./dist/global.cjs",
      "import": "./dist/global.js"
    },
    "./*": "./*"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "pnpm run build:generate && run-p build:build build:types",
    "build:generate": "tsx build/generate.ts",
    "build:build": "tsx build/build.ts",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "icon-svg": "workspace:*",
    "@pnpm/find-workspace-dir": "^4.0.3",
    "@pnpm/find-workspace-packages": "^4.0.41",
    "@pnpm/logger": "^4.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.8.3",
    "@types/prettier": "^2.7.1",
    "camelcase": "^7.0.0",
    "chalk": "^5.1.0",
    "consola": "^2.15.3",
    "esbuild": "^0.17.11",
    "esbuild-plugin-globals": "^0.1.1",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "tsx": "^3.12.5",
    "typescript": "^4.8.4",
    "unplugin-vue": "^3.1.3",
    "vue": "^3.2.47",
    "vue-tsc": "^1.0.0"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/types/*"
      ]
    }
  }
}
