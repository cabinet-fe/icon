{
  "name": "icon-ultra",
  "version": "4.0.0",
  "description": "Vue components of Icons collection.",
  "type": "module",
  "keywords": [
    "icon",
    "svg",
    "vue"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cabinet-fe/icon-ultra/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabinet-fe/icon-ultra.git",
    "directory": "packages/vue"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./colorful": {
      "import": "./dist/colorful.js",
      "require": "./dist/colorful.js",
      "types": "./dist/colorful.d.ts"
    },
    "./normal": {
      "import": "./dist/normal.js",
      "require": "./dist/normal.js",
      "types": "./dist/normal.d.ts"
    },
    "./source": "./src/index.ts",
    "./*": "./*"
  },
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "bun run build/index.ts"
  },
  "peerDependencies": {
    "vue": ">=^3.4.21"
  },
  "devDependencies": {
    "tsdown": "^0.12.8",
    "icon-svg": "workspace:*",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^24.0.3",
    "camelcase": "^7.0.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "prettier": "^3.5.3",
    "unplugin-vue": "^6.2.0",
    "vue": "^3.5.16",
    "vue-tsc": "^2.2.10"
  }
}
