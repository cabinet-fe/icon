{
  "compilerOptions": {
    "module": "ESNext",
    "baseUrl": ".",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "Bundler",
    "esModuleInterop": true,
    "jsx": "preserve",
    "target": "ESNext",
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "types": []
  },
  "include": ["./src/**/*"]
}
